                <img src="../img/Bg/watches-1.png" alt="">
            </div>
        </div>
    </section>
    <!-- -----------\* First Container End */----------- -->



    <!-- -----------Second Container----------- -->
    <section id="2">
        <div class="secondContainer">
            <div class="second_cont_firstPart" data-aos="fade-up"></div>
