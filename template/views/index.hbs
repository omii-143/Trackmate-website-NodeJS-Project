                    </div>
                </div>
            </div>
            <div class="second_cont_firstPart"></div>
        </div>
    </section>

    <!-- -----------\* Third Container End */----------- -->



    <!-- -----------Fourth Container----------- -->
    <section id="4">
        <div class="Container-1200px">
            <div class="fourthContainer_Background">
                <div id="4_1" class="fourth_middle-container">
                    <div class="contentFourth">
                        <img src="../img/Icon/eye.webp" alt="">
