                    </div>
                    <img src="../img/Bg/watches-3.webp" alt="">
                    <a href="" class="My-Btn Btn-20">Buy Now</a>
                </div>
            </div>
        </div>
    </section>
    <!-- -----------\* Fourth Container End */----------- -->


    <!-- -----------Fifth Container----------- -->

    <section id="5">
        <div class="fifthContainer">
            <div class="fifth_inner_container">
                <div class="IconcontentPart">
                    <div class="IconPart">
                        <img src="../img/Icon/alert.webp" alt="...">
