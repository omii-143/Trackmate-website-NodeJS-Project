    </section>

    <!-- -----------|* Eight Container End  *|----------- -->



    <!-- -----------Nine Container----------- -->

    <section id="9">
        <div class="NineContainer">
            <div class="nine_content">
                <h1>Subscribe and Get 15% Off Your First Purchase</h1>
                <form action="#" class="form">
                    <label for="signUp">Email*</label>
                    <div class="inputForm">
                        <input type="text" id="signUp" class="signupInput" placeholder="Sign Up Here">
                        <a href="#" class="subBtn">Subscribe</a>
                    </div>
                </form>
            </div>
        </div>
    </section>
    <!-- -----------|* Nine Container End  *|----------- -->


    <!-- -----------Footer Container----------- -->

    {{>footer}}

    <!-- -----------|* Footer Container End  *|----------- -->
    <script src="../js/script.js"></script>
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    <script>
        AOS.init({
            duration: 1800,
